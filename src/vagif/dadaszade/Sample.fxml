<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" style="-fx-background-color: #0E6655;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vagif.dadaszade.SampleController">
<children>
      <Button layoutX="39.0" layoutY="190.0" mnemonicParsing="false" onAction="#saveStudent" prefHeight="42.0" prefWidth="327.0" style="-fx-background-color: yellow;" text="Save">
         <font>
            <Font name="Britannic Bold" size="18.0" />
         </font></Button>
      <TextField fx:id="studentNameText" alignment="TOP_CENTER" layoutX="44.0" layoutY="39.0" prefHeight="31.0" prefWidth="313.0" promptText="Enter your name:" style="-fx-background-color: yellow;">
         <font>
            <Font name="Britannic Bold" size="15.0" />
         </font></TextField>
      <Label fx:id="studentNameLabel" layoutX="39.0" layoutY="104.0" prefHeight="50.0" prefWidth="313.0" textFill="WHITE">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
      <Button layoutX="39.0" layoutY="286.0" mnemonicParsing="false" onAction="#openLoginPage" prefHeight="42.0" prefWidth="327.0" style="-fx-background-color: #98E300;" text="Enter the Login">
         <font>
            <Font name="Britannic Bold" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
